
x = 0

def draw():
    global x
    x += 1
    if x <= 9:
        print "{}{}".format('00000', x)
    elif x <= 99:
        print "{}{}".format('0000', x)
    elif x <= 999:
        print "{}{}".format('000', x)
    elif x <= 9999:
        print "{}{}".format('00', x)
    elif x <= 99999:
        print "{}{}".format('0', x)
    elif x <= 999999:
        print "{}".format(x)
 




